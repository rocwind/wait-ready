{
  "name": "wait-ready",
  "version": "0.4.0",
  "description": "a utility for executing actions after some async tasks complete",
  "main": "lib/bundle.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "del-cli lib",
    "build": "npm run clean && npm run compile && rollup lib/index.js --file lib/bundle.js --format cjs",
    "compile": "tsc --outDir lib -d",
    "preversion": "npm test && npm run build",
    "postversion": "git push origin master && git push origin --tags && npm publish",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rocwind/wait-ready.git"
  },
  "author": "Roc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rocwind/wait-ready/issues"
  },
  "homepage": "https://github.com/rocwind/wait-ready#readme",
  "devDependencies": {
    "@types/jest": "25.1.3",
    "del-cli": "3.0.0",
    "jest": "25.1.0",
    "rollup": "1.31.1",
    "ts-jest": "25.2.1",
    "typescript": "3.8.2"
  }
}
